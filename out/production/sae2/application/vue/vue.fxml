<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderpane" prefHeight="640.0" prefWidth="960.0" style="-fx-background-color: #E1F9E9;" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controleur.Controleur">
   <bottom>
      <VBox fx:id="vbox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <Pane fx:id="pane" focusTraversable="true" BorderPane.alignment="CENTER">
         <children>
            <TilePane fx:id="tilepane" focusTraversable="true" onKeyPressed="#update" prefColumns="30" prefHeight="640.0" prefRows="20" prefTileHeight="32.0" prefTileWidth="32.0" prefWidth="960.0" />
         </children>
      </Pane>
   </center>
</BorderPane>
